.invenout{
  padding-bottom: 20px;line-height: 20px;
}
.inveninner{
  border: 1px solid #999;line-height: 20px
}
.inveninner view{
  padding: 2px;padding-left: 6px
}

.headerview{
  padding-bottom: 10px;
  background: rgb(215,34,100)
}
.headerview .dt_title{
  float:left;
  height:46px;
  line-height:46px;
  padding-left: 10px;
  padding-right: 10px;
  font-size: 19px
}
.headerview button{
  width: 103px;
  border-radius: 50px;
  border: 1px solid #999;
  background: #fff;
  margin-left: 0px
}



.middleview{
  
  border-top:1px solid #999;
  border-bottom: 1px solid #999;
}
.middleview ul li{
  padding:20px
}
.middleview ul li .screchinner{
  border:1px solid #666;
  height: 40px;
  line-height: 40px;
  border-radius: 50px;
}
.middleview ul li .screchinner_left{
  float:left;
  margin-right: 10px;
  width: 80px;
  text-align: center;
  border-right: 1px solid #000;
  color:red;
  font-size: 20px;
}
.middleview ul li .screchinner_right{
  overflow: hidden
}
.middleview ul li .screchinner_right input{
  overflow: hidden;
  height:40px;
  line-height: 40px;
}
.btninproduce{
  float:right;
  margin-top:-51px;
}
.btinproduce{
  padding-left:2px;
  padding-right:2px;
  border-radius: 20px;
  border: 1px solid #999;
  background: #fff;
  color: #999
}
.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.td {
  padding: 5px;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  width: 100%;
  vertical-align: middle;
  line-height: 46px;
  height: 46px;
}


.hidden{
  display: none
}



